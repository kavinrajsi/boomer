[
  {
    "name": "Boomer",
    "theme_name": "Vintage",
    "theme_version": "1.0.0",
    "theme_documentation_url": "https://sundarisilks.com",
    "theme_author": "Kavinraj",
    "theme_support_email": "sikavinraj@gmail.com"
  },
  {
    "name": "Navigation Bar Colors Settings",
    "settings": [
      {
        "type": "color",
        "id": "navbar_color",
        "label": "NavBar Background Color",
        "default": "#fff"
      }
    ]
  },
  {
    "name": "Typography",
    "settings": [
      {
        "type": "header",
        "content": "Heading Font Styles"
      },
      {
        "type": "font_picker",
        "id": "header_text_font",
        "label": "Font",
        "default": "mono"
      },
      {
        "type": "header",
        "content": "Body Font Styles"
      },
      {
        "type": "font_picker",
        "id": "body_text_font",
        "label": "Font",
        "default": "mono"
      },
      {
        "type": "header",
        "content": "Body Font Size"
      },
      {
        "type": "range",
        "id": "body_font_size",
        "label": "Font Size",
        "default": 16,
        "unit": "px",
        "min": 13,
        "max": 18,
        "step": 1
      }
    ]
  },
  {
    "name": "Favicon",
    "settings": [
      {
        "type": "image",
        "id": "favicon",
        "label": "Favicon"
      }
    ]
  }
]
