      html = html + '<tr><td class="strong">Measurement details of</td><td>' + measurementList[i].title + '</td></tr>';
      html = html + '<tr><td class="strong">Front Neck Design</td><td>' + measurementList[i].front_neck_designs + '</td></tr>';
      html = html + '<tr><td class="strong">Back Neck Design</td><td>' + measurementList[i].back_neck_designs + '</td></tr>';
      html = html + '<tr><td class="strong">Sleeve Design</td><td>' + measurementList[i].types_of_sleeves + '</td></tr>';
      html = html + '<tr><td class="strong">Opening</td><td>' + measurementList[i].openings + '</td></tr>';
      html = html + '<tr><td class="td_select_measurement"><button class="btn btn-default action_btn select_measurement_btn" value=' + i + '>Select</button></td><td><!--<button class="btn btn-default action_btn delete_measurement_btn" value=' + i + '>Delete</button>--></td></tr>';
      html = html + '</tbody></table></div></div></div></div>';
    }
    html = html + '</div>';
    measurementListSection$.show();
    measurementListSection$.html(html).fadeIn(500);
    $('.delete_measurement_btn').on('click', async function () {
      measurementList.splice(this.value, 1);
      if (!measurementList.length) {
        $('.no-measurement-section').show();
      }
      getMeasurementList();
//       await updateMeasurement();
    });

    $('.select_measurement_btn').on('click', function () {
   	  let path = window.location.search;
   	  if (!path) {
   	  	return;
   	  }
   	  if (path.split("=").length <= 1) {
   	  	return;
   	  }
      if (path.includes('?checkout_url') && !!measurementList[this.value]) {
      	const productValues = JSON.parse(localStorage.getItem('selectedProductValues'));
      	productValues['measurement'] = measurementList[this.value];
      	localStorage.setItem('selectedProductValues', JSON.stringify(productValues));
      	window.location = path.split("=")[1] + '?measurement_id=' + measurementList[this.value].id;
      }
    });

    let productLocation = window.location.search.substr(window.location.search.lastIndexOf('products/')).split('/') || [];
    let showSelectOption = false;

    if (productLocation.length > 1) {
  	  showSelectOption = !!productLocation[1];
    }

    setTimeout(() => {
      if (!showSelectOption) {
  	    $(".select_measurement_btn").remove();
      }
    }, 500);
  }

  function resetForm() {
    $('#measurementForm').trigger("reset");
    $("#blouse_design_selected").css('display', 'block').html('');
    $("#front_neck_design_selected").css('display', 'block').html('');
    $("#back_neck_design_selected").css('display', 'block').html('');
    $("#types_of_sleeve_design_selected").css('display', 'block').html('');
    $("#opening_design_selected").css('display', 'block').html('');
    homeSection$.show();
    $('.no-measurement-section').hide();
    $("#confirmation").removeClass('active');
    $("#parent_confirmation").removeClass('active');
    $("#parent_embroidery").addClass('active');
    $("#embroidery").addClass('active');
    measurementSection$.hide();
  }

  function encodeMeasurement(list) {
    let updatedMeasurementList = [];
    for (let i = 0; i < list.length; i++) {
      let keyValueObject = {};
      let keyValues = [];
      Object.keys(list[i]).forEach(key => {
        if (key !== 'id' && key !== 'type') {
          let object = {};
          object['key'] = key;
          object['keyLabel'] = capitalizeTheFirstLetterOfEachWord(key.replaceAll('_', ' '));
          object['value'] = list[i][key];
          object['valueLabel'] = capitalizeTheFirstLetterOfEachWord(list[i][key].replaceAll('-', ' '));
          keyValues.push(object);
        } else {
          keyValueObject[key] = list[i][key];
        }
      });
      keyValueObject['keyValues'] = keyValues;
      updatedMeasurementList.push(keyValueObject);
    }
    return updatedMeasurementList;
  }

  function decodeMeasurement(list) {
    let updatedMeasurementList = [];
    for (let i = 0; i < list.length; i++) {
      let object = {};
      Object.keys(list[i]).forEach(key => {
        if (key === 'id' || key === 'type') {
          object[key] = list[i][key];
        } else if (key === 'keyValues') {
          list[i]['keyValues'].forEach(keyValue => {
            object[keyValue['key']] = keyValue['value'];
          });
        }
      });
      updatedMeasurementList.push(object);
    }
    return updatedMeasurementList;
  }

  function capitalizeTheFirstLetterOfEachWord(words) {
   var separateWord = words.toLowerCase().split(' ');
   for (var i = 0; i < separateWord.length; i++) {
      separateWord[i] = separateWord[i].charAt(0).toUpperCase() +
      separateWord[i].substring(1);
   }
   return separateWord.join(' ');
}

</script>
