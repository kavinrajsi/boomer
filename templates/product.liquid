title : <h2>{{ product.title }}</h2>
content : <p>{{ product.content | | strip_html }}</p>
available : <p>{{ product.available }}</p>
price varies : <p>{{ product.compare_at_price_varies }}</p>
<!-- Featured Image : <p>{{ product.featured_image }}</p> -->
product media
{% for image in product.images %}
    <p><img src="{{ image.src | product_img_url: "large" }}"></p>
  {% endfor %}
  {% case media.media_type %}
  {% when 'image' %}
    <div class="product-single__media" style="padding-top: {{ 1 | divided_by: media.aspect_ratio | times: 100}}%;" data-media-id="{{ media.id }}">
      {{ media | image_url: width: 2048, height: 2048 | image_tag }}
    </div>
  {% when 'external_video' %}
    <div class="product-single__media" style="padding-top: {{ 1 | divided_by: media.aspect_ratio | times: 100}}%;" data-media-id="{{ media.id }}">
      {{ media | external_video_tag }}
    </div>
  {% when 'video' %}
    <div class="product-single__media" data-media-id="{{ media.id }}">
      {{ media | video_tag: controls: true }}
    </div>
  {% when 'model' %}
    <div class="product-single__media" style="padding-top: 100%" data-media-id="{{ media.id }}">
      {{ media | model_viewer_tag }}
    </div>
  {% else %}
    <div class="product-single__media" style="padding-top: 100%;" data-media-id="{{ media.id }}">
      {{ media | media_tag }}
    </div>
{% endcase %}
preview image
{% if product.media.size > 1 %}
  <div class="thumbnails-wrapper">
    {% for media in product.media %}
      <a data-thumbnail-id="{{ media.id }}">
        {{ media.preview_image | image_url: width: 110, height: 110, scale: 2 | image_tag: media.alt, 'product-single__thumbnail-image' }}
      </a>
    {% endfor %}
  </div>
{% endif %}


Price : <p>{{ product.price }}</p>
Price max : <p>{{ product.price_max }}</p>
Price min : <p>{{ product.price_min }}</p>
Price varies : <p>{{ product.price_varies }}</p>
Type : <p>{{ product.type }}</p>
